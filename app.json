{
  "expo": {
    "name": "Refill",
    "slug": "refill",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFEB3B"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.refill.app",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Refill needs your location to show nearby restaurants with free refills and amenities."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFEB3B"
      },
      "package": "com.refill.app",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": ["ACCESS_FINE_LOCATION", "ACCESS_COARSE_LOCATION"],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDQIbT3FENMDxFzOMNDf-aFq56TmTmSFuU"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Refill to use your location to find nearby restaurants."
        }
      ]
    ]
  }
}
